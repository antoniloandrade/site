}
.btn-primary:hover{
  background-color: #000;
  color: #FFF;
}
/*[split]*//*[[fetaures]]*/.fetaures{
  width: 100%;
  display: flex;
  justify-content: center;
}
.fetaures .fbox{
  width: 250px;
  height: 300px;
  position: relative;
  color: #FFF;
}
.fetaures .fbox:nth-child(odd){
  background-color: #640f0f11;
}
@media screen and (max-width: 768px){
  .fetaures .fbox{
    width: 90%;
  }
}
.fetaures .fbox .fbox-side1{
  position: absolute;
  bottom: 0px;
  left: 0px;
  height: 100%;
  width: 100%;
  padding: 10px;
  transition: 0.5s all;
}
.fetaures .fbox .s{
  opacity: 1;
  height: 100%;
  overflow: hidden;
  transition: 0.5s all;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:center;
  text-align: center
}
.fetaures .fbox .s i {
  color:  #640f0f!important;
  font-size: 35px;
  margin-bottom: 15px;
}
.fetaures .fbox .s h3 {
  color:  #000;
  font-size: 18px;
  font-weight: bold;
}
.fetaures .fbox:hover .s{
  opacity: 0;
  height: 5px;
  overflow: hidden;
}
.fetaures .fbox .description{
  opacity: 0;
  height: 5px;
  overflow: hidden;
  transition: 0.5s all;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:center;
  text-align: center
}
.fetaures .fbox:hover .description{
  opacity: 1;
  height: 100%;
  overflow: hidden;
}
.fetaures .fbox:hover .fbox-side1{
  position: absolute;
  bottom: 0px;
  left: 0px;
  height: 105%;
  width: 100%;
  opacity: 1;
  background: #640f0f;
  color: #FFF;
  padding: 10px;
}
/*[split]*//*[[depos]]*/.depos .cont{
  display: flex;
  border-top: 1px solid #EEE;
  border-bottom: 1px solid #EEE;
  padding: 25px 0;
}
.depos .sz1{
  font-size: 55px;
  color: #50D18E;
  padding: 5px;
}
.depos .sz2{
  display: flex;
  padding: 5px;
  justify-content: flex-start;
  align-items: center;
  padding: 5px;
  min-width: 40%;
}
.depos .sz2 img{
  margin: 10px;
}
.depos .sz2 .name{
  font-weight: bold;
}
.depos .sz2 .function{
  font-weight: 400;
}
.depos .sz3{
  display: flex;
  padding: 5px;
  justify-content: center;
  align-items: center;
}
/*[split]*//*[[plan]]*/.ug-lightbox{
  display: none;
}
.plan{
  width: 300px;
  margin: 10px;
  border: 2px solid #eee;
  min-height: 500px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  transition: 0.5s all;
}
.plan:hover{
  box-shadow: 0px 0px 10px #00000055;
  margin-top: -5px;
}
.plan .table-features{
  margin: 15px;
  margin-top: 22px;
}
.plan .featured{
  color: #000;
  font-size: 11px;
  text-transform: uppercase;
  background-color: #08F7D6;
  text-align: center;
  padding: 5px;
  letter-spacing: 5px;
  font-weight: bold;
  font-family: montserrat;
}
.plan td{
  padding: 5px;
  font-size: 14px;
}
.plan i{
  color: #999
}
.plan .title-box{
  display: flex;
  flex-direction: column;
  height: 230px;
  justify-content:center;
  align-items: center;
  color: #fff;
  text-shadow: 0px 0px 5px #000;
  padding: 15px;
  font-family: montserrat;
}
.plan .plan-name{
  font-weight: bold;
  font-size: 19px;
}
.plan .plan-price{
  display: flex;
  position: relative
}
.plan .price-symbol{
  font-size: 18px;
  top: 20px;
  position: relative;
  margin-right: 5px;
  font-family: montserrat;
  font-weight: bold;
}
.plan .price{
  font-size: 70px;
  font-weight: bold;
  color: #08F7D6;
  font-family: montserrat;
}
.plan .price-bef{
  top: 2px;
  font-family: montserrat;
  font-weight: bold;
  position: absolute;
}
.plan .price-after{
  font-size: 18px;
  position: relative;
  top: 60px;
  font-family: montserrat;
  font-weight: bold;
}
.plan .join-btn{
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 15px 0;
  border-top: 2px solid #BBB;
}
.plan p {
  font-size: 13px;
  padding: 15px;
  text-align: center;
}
